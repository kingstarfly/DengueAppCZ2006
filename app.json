{
  "expo": {
    "name": "Mozi",
    "slug": "mozi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashdengue.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "androidStatusBar": {
      "hidden": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kingstarfly.mozi-app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
